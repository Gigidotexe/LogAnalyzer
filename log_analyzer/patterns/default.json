{
  "Failed SSH login": {
    "pattern": "Failed password for (?P<user>\\w+) from (?P<ip>\\d+\\.\\d+\\.\\d+\\.\\d+) port (?P<port>\\d+)",
    "color": "RED",
    "severity": "high"
  },
  "Privilege escalation": {
    "pattern": "sudo: (?P<user>\\w+) : session opened for user root",
    "color": "MAGENTA",
    "severity": "critical"
  },
  "Invalid user": {
    "pattern": "Invalid user (?P<user>\\w+) from (?P<ip>\\d+\\.\\d+\\.\\d+\\.\\d+)",
    "color": "YELLOW",
    "severity": "medium"
  },
  "Successful SSH login": {
    "pattern": "Accepted password for (?P<user>\\w+) from (?P<ip>\\d+\\.\\d+\\.\\d+\\.\\d+)",
    "color": "WHITE",
    "severity": "info"
  }
}
